<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/parentTeamMember"
    android:layout_margin="10dp"
    app:cardCornerRadius="10dp"
    app:cardElevation="10dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:id="@+id/finalTeamMemberRel">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/teammember_placeholder"
            android:textSize="25sp"
            android:textColor="@color/maincolor"
            android:textStyle="bold"
            android:id="@+id/finalTeamMember"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sf_placeHolder"
            android:textSize="25sp"
            android:textColor="@color/maincolor"
            android:layout_alignParentEnd="true"
            android:id="@+id/finalTeamMemberStrength"/>

    </RelativeLayout>




</androidx.cardview.widget.CardView>